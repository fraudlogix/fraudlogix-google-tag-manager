params+= '&mm=' + encodeUriComponent(mm);
params+= '&os=' + encodeUriComponent(os);
params+= '&ua=' + encodeUriComponent(ua);
params+= '&lat=' + encodeUriComponent(lat);
params+= '&lon=' + encodeUriComponent(lon);
params+= '&idl=' + encodeUriComponent(idl);
